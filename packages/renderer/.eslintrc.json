{
  "env": {
    "browser": true,
    "node": false
  },
  "extends": [
    /** @see https://eslint.vuejs.org/rules/ */
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "vue/multi-word-component-names": 0
  }
}
